let g:clap_open_action = { 'ctrl-t': 'tab split', 'ctrl-s': 'split', 'ctrl-v': 'vsplit' }
let g:clap_disable_run_rooter = v:true
let g:clap_theme = 'material_design_dark'
map <Leader>ff :Clap files<CR>
map <Leader>fb :Clap buffers<CR>
map <Leader>fg :Clap grep<CR>
map <Leader>fh :Clap history<CR>
map <Leader>fc :Clap hist:<CR>
map <Leader>fi :Clap filer<CR>
