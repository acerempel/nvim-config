{
  "suggest.autoTrigger": "none",
  "diagnostic.virtualText": true,
  "git.branchCharacter": "⌥",
  "typescript.preferences.quoteStyle": "double",
  "javascript.preferences.quoteStyle": "double",
  "html.suggest.ionic": false,
  "html.suggest.angular1": false,
  "html.autoClosingTags": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "html.antlers"
  ],
  "tailwindCSS.htmlLanguages": [
    "blade",
    "edge",
    "eelixir",
    "ejs",
    "elixir",
    "elm",
    "erb",
    "eruby",
    "haml",
    "handlebars",
    "html",
    "htmldjango",
    "HTML (EEx)",
    "HTML (Eex)",
    "html.twig",
    "html.antlers",
    "jade",
    "leaf",
    "markdown",
    "njk",
    "nunjucks",
    "php",
    "razor",
    "slim",
    "svelte",
    "twig",
    "vue"
  ],
  "tailwindCSS.headwind.runOnSave": false,
  "hover.floatConfig": { "border": true },
  "eslint.packageManager": "pnpm",
  "suggest.enablePreselect": true,
  "suggest.numberSelect": false,
  "suggest.maxCompleteItemCount": 10,
  "suggest.snippetsSupport": true,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.infoSign": "--",
  "list.selectedSignText": "➤",
  "coc.preferences.extensionUpdateCheck": "weekly",
  "list.source.files.args": ["--hidden", "--no-require-git", "--iglob=!.git/", "--files"],
  "codeLens.enable": true,
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "filetypes": ["haskell", "lhaskell"],
      "rootPatterns": ["cabal.project", "cabal.project.*", "stack.yaml", "*.cabal"]
    },
    "elmLS": {
      "command": "./node_modules/@elm-tooling/elm-language-server/out/index.js",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmPath": "./node_modules/elm/bin/elm",
        "elmFormatPath": "./node_modules/elm-format/bin/elm-format",
        "elmTestPath": "./node_modules/elm-test/bin/elm-test"
      }
    }
  }
}
