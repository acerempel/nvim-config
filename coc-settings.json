{
  "coc.preferences.formatOnType": true,
  "suggest.autoTrigger": "trigger",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": " ■ ",
  "diagnostic.virtualTextLineSeparator": " ∣ ",
  "diagnostic.errorSign": "⊗",
  "diagnostic.infoSign": "⦿",
  "diagnostic.hintSign": "⊙",
  "git.branchCharacter": "⌥",
  "typescript.preferences.quoteStyle": "double",
  "javascript.preferences.quoteStyle": "double",
  "typescript.referencesCodeLens.enable": false,
  "html.suggest.ionic": false,
  "html.suggest.angular1": false,
  "html.autoClosingTags": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "html.antlers"
  ],
  "tailwindCSS.htmlLanguages": [
    "blade",
    "edge",
    "eelixir",
    "ejs",
    "elixir",
    "elm",
    "erb",
    "eruby",
    "haml",
    "handlebars",
    "html",
    "htmldjango",
    "HTML (EEx)",
    "HTML (Eex)",
    "html.twig",
    "html.antlers",
    "jade",
    "leaf",
    "markdown",
    "njk",
    "nunjucks",
    "php",
    "razor",
    "slim",
    "svelte",
    "twig",
    "vue"
  ],
  "tailwindCSS.headwind.runOnSave": false,
  "eslint.packageManager": "pnpm",
  "suggest.enablePreselect": true,
  "suggest.numberSelect": false,
  "suggest.maxCompleteItemCount": 7,
  "suggest.snippetsSupport": true,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.triggerCompletionWait": 50,
  "signature.floatConfig": {"maxWidth": 51, "maxHeight": 22},
  "diagnostic.refreshOnInsertMode": false,
  "list.indicator": "❯",
  "list.selectedSignText": "➤",
  "list.insertMappings": {
    "<C-p>": "do:previous",
    "<C-n>": "do:next",
    "∆": "toggle"
  },
  "coc.preferences.extensionUpdateCheck": "weekly",
  "list.source.files.args": ["-H", "-tf", "-tl", "-E.git", "-cnever"],
  "list.source.files.command": "fd",
  "codeLens.enable": true,
  "Lua.diagnostics.globals": ["vim"],
  "rust-analyzer.assist.importGranularity": "module",
  "rust-analyzer.lens.implementations": false,
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "filetypes": ["haskell", "lhaskell"],
      "rootPatterns": ["cabal.project", "stack.yaml", "*.cabal"]
    }
  }
}
